<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ 'main.wellcome'|trans }}{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block header %}{% include('header.html.twig') %}{% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}{% include('footer.html.twig') %}{% endblock %}
        {% block javascripts %}
            {% if js is defined %}
                <script>
                    var js = {};
                    {% for key, value in js %}
                        js["{{ key }}"] = "{{ value }}";
                    {% endfor %}
                </script>
            {% endif %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
